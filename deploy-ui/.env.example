# 环境配置示例文件
# 复制此文件为 .env 并根据需要修改配置

# 服务端口配置
FRONTEND_PORT=3000
BACKEND_PORT=3001
NGINX_PORT=80

# 文件上传配置
MAX_FILE_SIZE=5242880  # 5MB in bytes
ALLOWED_FILE_TYPES=image/png

# Android构建配置
ANDROID_COMPILE_SDK=34
ANDROID_BUILD_TOOLS=34.0.0

# 应用配置
APP_NAME=Android WebApp Builder
APP_VERSION=1.0.0

# 开发模式配置
NODE_ENV=production
DEBUG=false

# 日志配置
LOG_LEVEL=info
LOG_FILE=app.log